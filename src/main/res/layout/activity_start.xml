<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_start"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rilind.budget1.start">

    <EditText
        android:layout_width="0dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText10"
        android:hint="Price"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        app:layout_constraintLeft_toRightOf="@+id/editText12"
        android:layout_height="42dp"
        app:layout_constraintRight_toLeftOf="@+id/editText11"
        android:layout_marginEnd="2dp"
        android:layout_marginStart="2dp"
        android:textAlignment="textEnd" />

    <EditText
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText11"
        android:hint="Moms"
        android:layout_height="42dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="60dp"
        android:textAlignment="textEnd" />

    <EditText
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText12"
        android:hint="Item"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="42dp"
        android:layout_width="250dp" />

    <Button
        android:text="SAVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText13"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText13"
        android:hint="Comment"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText12"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.53" />

    <TextView
        android:text="BUY"
        android:layout_width="53dp"
        android:layout_height="24dp"
        android:id="@+id/textView2"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        android:layout_marginStart="2dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="18sp" />

    <TextView
        android:text="SELL"
        android:layout_width="53dp"
        android:layout_height="24dp"
        android:id="@+id/textView3"
        android:layout_marginStart="2dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="18sp" />

    <EditText
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText6"
        android:hint="Item"
        android:layout_height="42dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="250dp" />

    <EditText
        android:layout_width="0dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText3"
        android:hint="Price"
        android:layout_height="42dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText6"
        android:layout_marginStart="2dp"
        app:layout_constraintRight_toLeftOf="@+id/editText8"
        android:layout_marginEnd="2dp"
        android:textAlignment="textEnd" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText9"
        android:hint="Comment"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <EditText
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText8"
        android:hint="Moms"
        android:layout_height="42dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="60dp"
        android:textAlignment="textEnd" />

    <Button
        android:text="SAVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:text="Results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp" />

    <Button
        android:text="History"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/button4"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp" />

</android.support.constraint.ConstraintLayout>
