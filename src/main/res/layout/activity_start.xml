<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_start"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rilind.budget1.start">

    <EditText
        android:layout_width="97dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText10"
        android:hint="Price"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/editText12"
        android:layout_height="42dp" />

    <EditText
        android:layout_width="62dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText11"
        android:hint="Moms"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/editText10"
        android:layout_marginTop="40dp" />

    <EditText
        android:layout_width="198dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText12"
        android:hint="Item"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="42dp" />

    <Button
        android:text="SAVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText13"
        android:layout_marginStart="288dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:layout_width="369dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText13"
        android:hint="Comment"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText12"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="BUY"
        android:layout_width="53dp"
        android:layout_height="24dp"
        android:id="@+id/textView2"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="18sp" />

    <TextView
        android:text="SELL"
        android:layout_width="53dp"
        android:layout_height="24dp"
        android:id="@+id/textView3"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="18sp" />

    <EditText
        android:layout_width="198dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText6"
        android:hint="Item"
        android:layout_height="42dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:layout_width="97dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText3"
        android:hint="Price"
        android:layout_height="42dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/editText6"
        android:layout_marginStart="8dp" />

    <EditText
        android:layout_width="369dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText9"
        android:hint="Comment"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:layout_width="62dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText8"
        android:hint="Moms"
        android:layout_height="42dp"
        app:layout_constraintLeft_toRightOf="@+id/editText3"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="8dp" />

    <Button
        android:text="SAVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        android:layout_marginStart="288dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:text="Results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toBottomOf="@+id/editText13"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:text="History"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toBottomOf="@+id/editText13"
        android:layout_marginStart="24dp"
        app:layout_constraintLeft_toRightOf="@+id/button4" />

</android.support.constraint.ConstraintLayout>
